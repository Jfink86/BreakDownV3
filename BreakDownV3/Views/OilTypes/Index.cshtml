@model IEnumerable<BreakDownV3.OilType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OilTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OilTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OilTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.OilTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OilTypeID })
        </td>
    </tr>
}

</table>
